{
    "name": "hinos-service-quickstart",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "preinstall": "npm install -g concurrently typescript cross-var tslint-config-standard",
        "all": "npm run clone && npm install && npm run build",
        "clone": "npm run clone:script && npm run clone:monitor && npm run clone:gateway && npm run clone:oauth && npm run clone:files && npm run clone:log && npm run clone:mail && npm run clone:web",
        "install": "npm run install:script && npm run install:monitor && npm run install:gateway && npm run install:oauth && npm run install:files && npm run install:log && npm run install:mail && npm run install:web",
        "build": "cd auto-script && tsc && cd ../api-monitor && tsc && cd ../api-gateway && tsc && cd ../files && tsc && cd ../log && tsc && cd ../mail && tsc && cd ../oauth && tsc",
        "start": "concurrently \"npm run start:oauth\" \"npm run start:files\" \"npm run start:log\" \"npm run start:mail\" \"npm run start:monitor\" \"npm run start:gateway\" \"npm run start:script\"",
        "pm2": "npm run pm2:oauth && npm run pm2:files && npm run pm2:log && npm run pm2:mail && npm run pm2:gateway && npm run pm2:monitor && npm run pm2:script",
        "pull": "npm run pull:script && npm run pull:monitor && npm run pull:gateway && npm run pull:oauth && npm run pull:files && npm run pull:log && npm run pull:mail && npm run pull:web",
        "update": "npm run update:script && npm run update:monitor && npm run update:gateway && npm run update:oauth && npm run update:files && npm run update:log && npm run update:mail",
        "newservice": "cross-var git clone https://github.com/doanthuanthanh88/hinos-framework.git -b service@template ",
        "start:web": "cd service-portal-manager && npm run dev",
        "start:gateway": "cd api-gateway && npm start",
        "update:gateway": "cd api-gateway && npm update --save",
        "pull:gateway": "cd api-gateway && git pull",
        "clone:gateway": "git clone https://github.com/doanthuanthanh88/hinos-framework.git -b api-gateway api-gateway",
        "install:gateway": "cd api-gateway && npm install",
        "pm2:gateway": "cd api-gateway && npm run pm2",
        "start:monitor": "cd api-monitor && npm start",
        "update:monitor": "cd api-monitor && npm update --save",
        "pull:monitor": "cd api-monitor && git pull",
        "clone:monitor": "git clone https://github.com/doanthuanthanh88/hinos-framework.git -b api-monitor api-monitor",
        "install:monitor": "cd api-monitor && npm install",
        "pm2:monitor": "cd api-monitor && npm run pm2",
        "start:script": "cd auto-script && npm start",
        "update:script": "cd auto-script && npm update --save",
        "pull:script": "cd auto-script && git pull",
        "clone:script": "git clone https://github.com/doanthuanthanh88/hinos-framework.git -b auto-script auto-script",
        "install:script": "cd auto-script && npm install",
        "pm2:script": "cd auto-script && npm run pm2",
        "start:oauth": "cd oauth && npm start",
        "update:oauth": "cd oauth && npm update --save",
        "pull:oauth": "cd oauth && git pull",
        "clone:oauth": "git clone https://github.com/doanthuanthanh88/hinos-framework.git -b oauth oauth",
        "install:oauth": "cd oauth && npm install",
        "pm2:oauth": "cd oauth && npm run pm2",
        "start:files": "cd files && npm start",
        "update:files": "cd files && npm update --save",
        "pull:files": "cd files && git pull",
        "clone:files": "git clone https://github.com/doanthuanthanh88/hinos-framework.git -b files files",
        "install:files": "cd files && npm install",
        "pm2:files": "cd files && npm run pm2",
        "start:log": "cd log && npm start",
        "update:log": "cd log && npm update --save",
        "pull:log": "cd log && git pull",
        "clone:log": "git clone https://github.com/doanthuanthanh88/hinos-framework.git -b log log",
        "install:log": "cd log && npm install",
        "pm2:log": "cd log && npm run pm2",
        "start:mail": "cd mail && npm start",
        "update:mail": "cd mail && npm update --save",
