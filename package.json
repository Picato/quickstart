{
  "name": "hinos-service-quickstart",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "once": "sudo npm install -g concurrently typescript cross-var rimraf tslint-config-standard",
    "all": "npm run clone && npm install && npm run build",
    "clone": "concurrently \"npm run clone:dynamic\" \"npm run clone:script\" \"npm run clone:monitor\" \"npm run clone:gateway\" \"npm run clone:oauth\" \"npm run clone:files\" \"npm run clone:log\" \"npm run clone:mail\" \"npm run clone:web\"",
    "postinstall": "npm run install:dynamic && npm run install:script && npm run install:monitor && npm run install:gateway && npm run install:oauth && npm run install:files && npm run install:log && npm run install:mail && npm run install:web",
    "build:services": "concurrently \"npm run build:oauth\" \"npm run build:files\" \"npm run build:mail\" \"npm run build:log\" \"npm run build:script\" \"npm run build:dynamic\" \"npm run build:monitor\" \"npm run build:gateway\"",
    "build": "npm run build:services && npm run build:web",
    "start": "concurrently \"npm run start:oauth\" \"npm run start:files\" \"npm run start:mail\" \"npm run start:log\" \"npm run start:script\" \"npm run start:dynamic\" \"npm run start:monitor\" \"npm run start:gateway\"",
    "pm2": "npm run pm2:oauth && npm run pm2:files && npm run pm2:mail && npm run pm2:log && npm run pm2:script && npm run pm2:dynamic && npm run pm2:monitor && npm run pm2:gateway && npm run pm2:web",
    "pull": "concurrently \"npm run pull:script\" \"npm run pull:dynamic\" \"npm run pull:monitor\" \"npm run pull:gateway\" \"npm run pull:oauth\" \"npm run pull:files\" \"npm run pull:log\" \"npm run pull:mail\" \"npm run pull:web\"",
    "update": "concurrently \"npm run update:script\" \"npm run update:dynamic\" \"npm run update:monitor\" \"npm run update:gateway\" \"npm run update:oauth\" \"npm run update:files\" \"npm run update:log\" \"npm run update:mail\"",
    "newservice": "cross-var git clone https://github.com/doanthuanthanh88/hinos-framework.git --single-branch -b service@template ",
    "start:web": "cd service-portal-manager && npm run dev",
    "pull:web": "cd service-portal-manager && git pull",
    "clone:web": "git clone https://github.com/doanthuanthanh88/vue-framework.git --single-branch -b service@portal service-portal-manager",
    "install:web": "cd service-portal-manager && npm install",
    "build:web": "cd service-portal-manager && npm run build",
    "pm2:web": "cd service-portal-manager && npm run pm2",
    "start:dynamic": "cd dynamic && npm start",
    "update:dynamic": "cd dynamic && npm update --save",
    "pull:dynamic": "cd dynamic && git pull",
