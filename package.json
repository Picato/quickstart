    "build:monitor": "cd api-monitor && npm run build",
    "pm2:monitor": "cd api-monitor && npm run pm2",
    "start:script": "cd auto-script && npm start",
    "update:script": "cd auto-script && npm update --save",
    "pull:script": "cd auto-script && git pull",
    "clone:script": "git clone https://github.com/doanthuanthanh88/hinos-framework.git --single-branch -b auto-script auto-script",
    "install:script": "cd auto-script && npm install",
    "build:script": "cd auto-script && npm run build",
    "pm2:script": "cd auto-script && npm run pm2",
    "start:oauth": "cd oauth && npm start",
    "update:oauth": "cd oauth && npm update --save",
    "pull:oauth": "cd oauth && git pull",
    "clone:oauth": "git clone https://github.com/doanthuanthanh88/hinos-framework.git --single-branch -b oauth oauth",
    "install:oauth": "cd oauth && npm install",
    "build:oauth": "cd oauth && npm run build",
    "pm2:oauth": "cd oauth && npm run pm2",
    "start:files": "cd files && npm start",
    "update:files": "cd files && npm update --save",
