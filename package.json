    "clone:files": "git clone https://github.com/doanthuanthanh88/hinos-framework.git --single-branch -b files files",
    "install:files": "cd files && npm install",
    "build:files": "cd files && npm run build",
    "pm2:files": "cd files && npm run pm2",
    "start:log": "cd log && npm start",
