        "pm2:mail": "cd mail && npm run pm2",
        "pull:web": "cd service-portal-manager && git pull",
        "clone:web": "git clone https://github.com/doanthuanthanh88/vue-framework.git -b service@portal service-portal-manager",
        "install:web": "cd service-portal-manager && npm install"
    },
